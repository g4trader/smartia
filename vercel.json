{
  "version": 2,
  "builds": [
    {
      "src": "apps/web/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "apps/web/$1"
    }
  ],
  "framework": "nextjs",
  "env": {
    "NEXT_PUBLIC_API_BASE": "https://smartia-api-642830139828.us-central1.run.app"
  }
}